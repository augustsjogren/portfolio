{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "env": { "browser": true, "node": true },
  "rules": { "no-console": "error", "react/prop-types": "off" }
}
